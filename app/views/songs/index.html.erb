<h1> <%= @user.name.titleize %> Songs </h1>
<% if @any_songs %>
  <table>
    <thead>
      <th>Songs</th>
    </thead>
    <tbody>
      <% @songs.each do |song| %>
        <tr>
          <td><%= song.title %></td>
        <tr>
    </tbody>
<% end %>
  </table>
<% else %>
  <div> No Songs for this band </div> 
<% end %>
<div> <%= link_to "See other bands", users_path %> </div>
